<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FixedAssets" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/blue</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.FixedAssets.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="SmallTextView" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <!--        <item name="fontFamily">@font/proxima_nova_regular</item>-->
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="Theme.FixedAssets.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.FixedAssets.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>